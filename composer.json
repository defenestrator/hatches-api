{
  "name": "defenestrator/hatches-api",
  "description": "On-stream fly fishing tools API",
  "keywords": [
    "Fly fishing",
    "API"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/framework": "~5",
    "league/glide":      "~0.3.1",
    "spatie/laravel-glide": "~2.0.0",
    "league/fractal": "0.12.*@dev",
    "sorskod/larasponse": "dev-L5",
    "sami/sami": "~3.0.2",
    "symfony/yaml": "~2.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.5.0",
    "whatthejeff/emoji-phpunit-resultprinter": "1.0.*@dev",
    "whatthejeff/fab": "~1.0",
    "phpspec/phpspec": "~2.1",
    "phpspec/nyan-formatters": "1.*",
    "phpspec/prophecy": "~1.3.1",
    "behat/behat": "~3.0@dev",
    "behat/mink": "~1.7@dev",
    "behat/mink-extension": "~2.0@dev",
    "laracasts/behat-laravel-extension": "dev-master#205a3d2",
    "laracasts/testdummy": "dev-master#44807c5",
    "barryvdh/laravel-ide-helper": "dev-master#1f5e436",
    "doctrine/dbal": "~2.6@dev",
    "fzaninotto/faker": "~1.4"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "Hatches\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php",
      "spec/",
      "features/"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "source"
  },
  "minimum-stability": "dev"
}
